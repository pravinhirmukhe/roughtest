/* website all css styles
------------------------------------------background:#f0f0f0;-------- */
html {position: relative;min-height: 100%;background:#ebebeb; }
body {/* Margin bottom by footer height */ margin-bottom:0px; font-family:'Quicksand', Helvetica, sans-serif;}
code {font-size: 80%;}
img{ }
body > .container {padding: 50px 10px 0;}

.list-group .list-group-item{border-color:#eee;color:#fc6f4b;padding:9px;}
.list-special .list-group-item {border-color:#f8f8f8;color:#fc6f4b;}
.list-group .list-group-item:hover{background:#f9f9f9;color:#fc6f4b;}
.list-group .list-group-item:focus{background:#f9f9f9;color:#fc6f4b;}
.list-group > a > .menu_icon{box-shadow: 
        0 2px 8px rgba(0,0,0,0.5), /* Exterior Shadow */
        inset 0 1px rgba(255,255,255,0.3), /* Top light Line */
        inset 0 10px rgba(255,255,255,0.2), /* Top Light Shadow */
        inset 0 10px 20px rgba(255,255,255,0.25), /* Sides Light Shadow */
        inset 0 -15px 30px rgba(0,0,0,0.3); /* Dark Background */float:left;background:#fc6f4b;height: 35px;width: 40px;margin-top:-9px;margin-left:-9px;}

.footer {position:absolute;width:100%;bottom:0;margin-bottom:0px;border-top:1px #c8c8c8  solid;/* Set the fixed height of the footer here */height:50px !important;color:#eee;background-color: #fff;}
.footer_line{min-height:25px !important;background:#37a8df;}
.footer > .container {padding-right: 15px;padding-left: 15px;}

/*navbar*/
.navbar-brand >img{margin-left:130px; margin-top:-10px;background: transparent;}
.hidden_side_menu_p{margin-top:5px;margin-left:-36%;}
/*ends*/
.error{color:#ff6666;padding:0px;margin-top:-70px;background:transparent;border-color:#eee;color:111;}
/* Custom page CSS
-------------------------------------------------- */
.panel_custom{border-color:#fc6f4b;position: relative;}
.panel_custom > .panel-heading {background: #fc6f4b;color:#fff;}
.panel_custom > .panel-title {color:#fff;}
/*profile panel*/
.panel_profile{border-color:#42c6dd}
.panel_profile > .panel-heading {background: #4ac9df;color:#fff;}
.panel_profile > .panel-title {color:#fff;}

.small_icon{height: 30px; width: 30px;}
/*f_request*/

/*model*/
.modal-content{border-top:solid 2px #febf10;border-bottom:2px solid #37a8df;border-left:2px solid #fc6f4b;border-right:2px solid #25af60;}
.modal-header{border-bottom:2px solid #37a8df;}
/*ends*/
.request{padding:10px;background: #fff;}
.request:hover{background: #5b5b5b;color:#fff;}
.r_option{border-radius: 3px;box-shadow: inset 0 0 10px #eee;padding:5px;}
/*end*/
/*ends*/
/* Placeholder dashboard ideas*/
.main {padding: 0px;}
.main .page-header {margin-top: 10px;}
.placeholders {margin-bottom: 10px;text-align: center;}
.placeholders h4 {margin-bottom: 0;}
.placeholder {margin-bottom: -5px;border: none;}
.placeholder img {display: inline-block;border-radius: 10%;}
.placeholder >a {border: none;background: transparent; -webkit-transition: all 0.6s;
    transition: all 0.6s;}
.placeholder >a:hover {border: none;background:#ebebeb;}
